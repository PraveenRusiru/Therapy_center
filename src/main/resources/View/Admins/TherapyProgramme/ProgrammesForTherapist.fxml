<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vBox" prefHeight="718.0" prefWidth="1058.0" spacing="20.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.therapy_center.Controller.Admin.ProgrammesForTherapistConrtoller">
   <children>
      <VBox prefHeight="0.0" prefWidth="1038.0" spacing="5.0" VBox.vgrow="ALWAYS">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="47.0" prefWidth="1038.0">
               <children>
                  <Pane prefHeight="47.0" prefWidth="401.0">
                     <children>
                        <Label layoutY="15.0" prefWidth="150.0" text="Session Id" />
                        <Label fx:id="sessionId" layoutX="128.0" layoutY="15.0" prefWidth="150.0" text="00" />
                     </children>
                  </Pane>
                  <Label fx:id="TherapistId" prefHeight="39.0" prefWidth="285.0" />
                  <Pane prefHeight="47.0" prefWidth="401.0">
                     <children>
                        <Label layoutX="21.0" layoutY="15.0" prefWidth="150.0" text="Therapist Id" />
                        <ComboBox fx:id="comboboxTherapistId" layoutX="222.0" layoutY="11.0" onAction="#selectingTherapist" prefHeight="26.0" prefWidth="148.0" />
                     </children>
                  </Pane>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="39.0" prefWidth="1038.0">
               <children>
                  <Label prefWidth="150.0" text="Therapy Programmes" HBox.hgrow="ALWAYS" />
                  <ComboBox fx:id="comboboxprogramme" prefHeight="16.0" prefWidth="150.0" />
                  <Pane prefHeight="39.0" prefWidth="680.0">
                     <children>
                        <Pane layoutX="298.0" layoutY="-2.0" prefHeight="47.0" prefWidth="453.0">
                           <children>
                              <Label layoutX="26.0" layoutY="15.0" prefWidth="150.0" text="Availabilities" />
                              <ComboBox fx:id="comboxAvailability" layoutX="108.0" layoutY="11.0" prefHeight="26.0" prefWidth="337.0" />
                           </children>
                        </Pane>
                     </children></Pane>
                  <Label prefWidth="150.0" text="Availabilities" textFill="#ffffff00" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="1038.0" spacing="30.0">
               <children>
                  <Button fx:id="btnReset" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnResetOnAction" prefWidth="80.0" style="-fx-background-color: #e74c3c;" text="Reset" textFill="WHITE" HBox.hgrow="ALWAYS">
                     <cursor>
                        <Cursor fx:constant="CLOSED_HAND" />
                     </cursor>
                  </Button>
                  <Button fx:id="btnDelete" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnDeleteOnAction" prefWidth="80.0" style="-fx-background-color: #c0392b;" text="Delete" textFill="WHITE" HBox.hgrow="ALWAYS">
                     <cursor>
                        <Cursor fx:constant="CLOSED_HAND" />
                     </cursor>
                  </Button>
                  <Button fx:id="btnUpdate" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnUpdateOnAction" prefWidth="80.0" style="-fx-background-color: #e67e22;" text="Update" textFill="WHITE" HBox.hgrow="ALWAYS">
                     <cursor>
                        <Cursor fx:constant="CLOSED_HAND" />
                     </cursor>
                  </Button>
                  <Button fx:id="btnSave" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnSaveCustomerOnAction" prefHeight="26.0" prefWidth="234.0" style="-fx-background-color: #2ecc71;" text="Save" textFill="WHITE">
                     <cursor>
                        <Cursor fx:constant="CLOSED_HAND" />
                     </cursor>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
      <TableView fx:id="tblTherapistForProgramme" onMouseClicked="#onClickTable" prefHeight="107.0" prefWidth="1038.0" VBox.vgrow="ALWAYS">
         <columns>
            <TableColumn fx:id="colId" prefWidth="75.0" text="Id" />
            <TableColumn fx:id="colTherapistId" prefWidth="75.0" text="Therapist  Id" />
            <TableColumn fx:id="colProgramme" prefWidth="75.0" text="Therapy Programme" />
            <TableColumn fx:id="colDay" prefWidth="75.0" text="Day" />
            <TableColumn fx:id="colTIme" prefWidth="75.0" text="Start TIme" />
            <TableColumn fx:id="colEndTIme" prefWidth="75.0" text="End TIme" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Label prefWidth="150.0" text="Check Availabilities">
         <font>
            <Font name="Apple LiGothic Medium" size="21.0" />
         </font>
      </Label>
      <VBox prefHeight="278.0" prefWidth="1038.0">
         <children>
            <HBox alignment="CENTER_LEFT">
               <children>
                  <Label prefWidth="150.0" text="Day" HBox.hgrow="ALWAYS" />
                  <ComboBox fx:id="comboboxCheckDay" prefHeight="16.0" prefWidth="150.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="1038.0">
               <children>
                  <Label prefWidth="150.0" text="Start Time" HBox.hgrow="ALWAYS" />
                  <ComboBox fx:id="cbCheckStartHH" editable="true" prefHeight="26.0" prefWidth="61.0" promptText="hh" />
                  <Label prefHeight="17.0" prefWidth="36.0" text="      :" />
                  <ComboBox fx:id="cbCheckStartMM" editable="true" prefHeight="26.0" prefWidth="64.0" promptText="mm" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="39.0" prefWidth="1038.0">
               <children>
                  <Label prefWidth="150.0" text="End TIme" HBox.hgrow="ALWAYS" />
                  <ComboBox fx:id="cbCheckEndHH" editable="true" prefHeight="26.0" prefWidth="61.0" promptText="hh" />
                  <Label prefHeight="17.0" prefWidth="36.0" text="      :" />
                  <ComboBox fx:id="cbCheckEndMM" editable="true" prefHeight="26.0" prefWidth="64.0" promptText="mm" />
                  <Pane prefHeight="28.0" prefWidth="505.0">
                     <children>
                        <Button fx:id="btnSearch" layoutX="113.0" layoutY="5.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnSearchOnAction" prefHeight="13.0" prefWidth="76.0" style="-fx-background-color: #2ecc71;" text="Search" textFill="WHITE">
                           <cursor>
                              <Cursor fx:constant="CLOSED_HAND" />
                           </cursor>
                        </Button>
                        <Button fx:id="btnCheckReset" layoutX="113.0" layoutY="5.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnCheckResetOnAction" prefHeight="13.0" prefWidth="76.0" style="-fx-background-color: #e74c3c;" text="Reset" textFill="WHITE">
                           <cursor>
                              <Cursor fx:constant="CLOSED_HAND" />
                           </cursor>
                        </Button>
                     </children>
                  </Pane>
               </children>
            </HBox>
            <TableView fx:id="tblAvailability" onMouseClicked="#onClickTable" prefHeight="178.0" prefWidth="1038.0">
               <columns>
                  <TableColumn fx:id="colCheckTherapistId" prefWidth="75.0" text="Therapist  Id" />
                  <TableColumn fx:id="colCheckStart" prefWidth="75.0" text="Start Time" />
                  <TableColumn fx:id="colCheckEndTime" prefWidth="75.0" text="End Time" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
